{
  "name": "bosta-insight-hub",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "vite",
    "dev:backend": "tsx watch server/index.ts",
    "build": "vite build",
    "build:backend": "tsc -p server/tsconfig.json",
    "lint": "npx eslint .",
    "preview": "vite preview",
    "start": "node dist/server/index.js",
    "cache:clear": "node server/scripts/clear-app-cache.js",
    "cache:dry-run": "node server/scripts/clear-app-cache.js --dry-run",
    "redis:clear": "node server/scripts/redis-manager.js clear",
    "redis:clear:queries": "node server/scripts/redis-manager.js clear queries",
    "redis:clear:filters": "node server/scripts/redis-manager.js clear filters",
    "redis:clear:sql": "node server/scripts/redis-manager.js clear sql",
    "redis:inspect": "node server/scripts/redis-manager.js inspect",
    "redis:warmup": "node server/scripts/redis-manager.js warmup"
  },
  "dependencies": {
    "@types/pg": "^8.15.4",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "canvas": "^3.1.2",
    "chart.js": "^4.4.3",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-winston": "^4.2.0",
    "google-auth-library": "^10.1.0",
    "googleapis": "^142.0.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "mongodb": "^6.8.0",
    "mysql2": "^3.10.1",
    "openai": "^4.53.0",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.16.3",
    "prismjs": "^1.29.0",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.26.1",
    "react-syntax-highlighter": "^15.5.0",
    "redis": "^4.6.14",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-winston": "^3.0.4",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.20",
    "@types/node": "^24.0.10",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "tsx": "^4.16.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
